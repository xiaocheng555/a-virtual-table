{
  "name": "a-virtual-table",
  "version": "1.0.0",
  "description": "The virtual scrolling component developed based on the Table component of Ant Design supports dynamic height and solves the problem of scrolling stuck when the amount of data is large.",
  "scripts": {
    "build:umd": "rollup --config ./build/rollup.config.umd.js",
    "build:browser": "rollup --config ./build/rollup.config.browser.js",
    "build:es": "rollup --config ./build/rollup.config.es.js",
    "build": "npm run build:umd && npm run build:browser && npm run build:es"
  },
  "keywords": [
    "vue",
    "a-virtual-table",
    "virtual",
    "table",
    "a-table",
    "ant-design"
  ],
  "author": "xiaocheng555",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^6.0.3",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/babel-preset-jsx": "^1.4.0",
    "@vue/compiler-sfc": "^3.1.1",
    "autoprefixer": "^8.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.4.1",
    "rollup": "^2.36.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "lodash": "^4.17.21"
  }
}
